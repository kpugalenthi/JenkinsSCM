pipeline {
  agent any

  stages {
    stage('Checkout') {
      steps {
        sh 'echo "Checkout"'
      }
    }   
     stage('Install') {
      steps {
        sh 'echo "Install"'
      }
    }  
     stage('Build') {
      steps {
        sh 'echo "Build"'
      }
    }  
     stage('Publish') {
      steps {
        sh 'echo "Publish"'
      }
    }  
  }
}